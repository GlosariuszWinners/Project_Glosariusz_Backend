        "express-rate-limit": "^5.5.1",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mongoose": "^6.1.0",
        "mongoose-url-slugs": "^1.0.2",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local-mongoose": "^6.1.0",
        "qs": "^6.10.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.5",
        "@babel/node": "^7.16.5",
        "@babel/plugin-proposal-optional-chaining": "^7.16.5",
        "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
